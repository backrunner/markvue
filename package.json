    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-vue": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-config-prettier": "^8.4.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vite-plugin-dts": "^0.9.9",
    "vue-eslint-parser": "^8.3.0",
